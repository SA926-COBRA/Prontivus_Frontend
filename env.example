# Prontivus Frontend Environment Variables

# API Configuration - Using deployed backend
VITE_API_URL=https://prontivus-backend-pa1e.onrender.com

# Application Configuration
VITE_APP_NAME=Prontivus
VITE_APP_VERSION=1.0.0

# Development Configuration
VITE_DEBUG=false
VITE_LOG_LEVEL=warn

# Feature Flags
VITE_ENABLE_TELEMEDICINE=true
VITE_ENABLE_TISS_BILLING=true
VITE_ENABLE_MOBILE_APP=true

# External Services (Optional)
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=

# File Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Security Configuration
VITE_SESSION_TIMEOUT=1800000
VITE_REFRESH_TOKEN_INTERVAL=900000
